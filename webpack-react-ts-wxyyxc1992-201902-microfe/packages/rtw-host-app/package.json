{
  "author": "wxyyxc1992@github",
  "description": "@wx/rtw-host-app",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "@wx/rtw-host-app",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/fe-boilerplate"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:sa": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.umd.dev.js --hot",
    "dev:sa": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "start": "npm run dev"
  },
  "dependencies": {
    "@wx/rtw-core": "*",
    "antd": "^3.12.1",
    "connected-react-router": "^6.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux-actions": "^2.6.4",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "smoothscroll-polyfill": "^0.4.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "23.3.12",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-actions": "^2.3.1",
    "@types/smoothscroll-polyfill": "^0.3.0",
    "autoprefixer": "9.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "enzyme": "^3.8.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "json-server": "0.14.2",
    "lerna": "^3.8.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.6.3",
    "react-router-dom": "^4.3.1",
    "resolve-url-loader": "3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.28.3",
    "webpack-cli": "3.2.1",
    "webpack-dashboard": "2.0.0",
